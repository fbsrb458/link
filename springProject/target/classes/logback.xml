<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
		<pattern>%d %5p [%c] %m%n</pattern>		
		</encoder>
	</appender>
	<logger name="com.myweb.www" lever="info" appender.ref="STDOUT"></logger>
	<logger name="org.springframework" lever="info" appender.ref="STDOUT"></logger>
	<logger name="org.springframework.jdbc" lever="info" appender.ref="STDOUT"></logger>
	<logger name="jdbc.splonty" lever="info" appender.ref="STDOUT"></logger>
	<logger name="jdbc.resultsettable" lever="info" appender.ref="STDOUT"></logger>
	
	<root level="info">
		<appender.ref ref="STDOUT" />
	</root>
</configuration>